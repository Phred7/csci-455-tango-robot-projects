<Widgets>:
    Button:
        text: "Press me"
        on_release: root.btn()

<SpeechPopup>:
    Label:
        text: "Select one"
        size_hint: 0.6, 0.2
        pos_hint: {"x":0.2, "top":1}

    GridLayout:
        # Set number of column
        rows:2
        row_force_default:True
        row_default_height:80
        pos_hint: {"x":0, "y":-.1}

        # create checkboxes
        Label:
            text: "Input"
            pos_hint: {"x":0.2, "top":1}


        CheckBox:
            # When check box selected, it will call checkboxes_click() method
            on_active: root.checkboxes_click(self, self.active, "Input")

            size_hint: 0.5, 0.5

        Label:
            text: "Output"
            pos_hint: {"x":-0.2, "top":1.2}

        CheckBox:
            # When check box selected, it will call checkboxes_click() method
            on_active: root.checkboxes_click(self, self.active, "Output")
            pos_hint: {"x":-0.1, "y":1.2}
            size_hint: 0.5, 0.5

    Button:
        text: "Record Speech"
        size_hint: 0.8, 0.2
        pos_hint: {"x":0.1, "y":.3}
        on_press: root.tts(self)

    Button:
        text: "Enter"
        size_hint: 0.8, 0.2
        pos_hint: {"x":0.1, "y":0.1}
        on_press: root.button_press(self)

